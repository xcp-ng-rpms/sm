From 8345971f68b5bbab89836e1caf5b8c0108d6c300 Mon Sep 17 00:00:00 2001
From: Damien Thenot <damien.thenot@vates.tech>
Date: Wed, 2 Jul 2025 15:52:40 +0200
Subject: [PATCH] cowutil: add a comment for isCoalesceableOnRemote

Signed-off-by: Damien Thenot <damien.thenot@vates.tech>
---
 drivers/cowutil.py | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/drivers/cowutil.py b/drivers/cowutil.py
index c4856102..3c2aa463 100755
--- a/drivers/cowutil.py
+++ b/drivers/cowutil.py
@@ -266,6 +266,8 @@ class CowUtil(ABC):
     def setKey(self, path: str, key_hash: str) -> None:
         pass
 
+    # The availability of coalesceOnline and cancelCoalesceOnline are dependent on isCoalesceableOnRemote() returning True
+    # If not, both function should raise NotImplementedError
     @abstractmethod
     def isCoalesceableOnRemote(self) -> bool:
         pass
